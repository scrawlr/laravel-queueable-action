{
    "name": "scrawlr/laravel-queueable-action",
    "description": "Queueable action support in Laravel",
    "keywords": [
        "spatie",
        "laravel-queueable-action"
    ],
    "homepage": "https://github.com/spatie/laravel-queueable-action",
    "license": "MIT",
    "authors": [
        {
            "name": "Brent Roose",
            "email": "brent@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        },
        {
            "name": "Alex Vanderbist",
            "email": "alex@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        },
        {
            "name": "Sebastian De Deyne",
            "email": "sebastian@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        },
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "laravel/lumen-framework": "^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^8.4|^9.3"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\QueueableAction\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\QueueableAction\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\QueueableAction\\QueueableActionServiceProvider"
            ]
        }
    }
}
